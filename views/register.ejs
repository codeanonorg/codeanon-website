<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" type="text/css" href="css/style.css">

    <!-- crypto lib -->
    <script type="text/javascript" src="crypto-js/crypto-js.js"></script>
    <script type="text/javascript">

        var encrypted = CryptoJS.SHA256("bbb");
        console.log(encrypted.toString())
    </script>

    <!-- main script -->
    <script type="text/javascript" src="js/script.js"></script>

    <title>CodeAnon</title>
</head>

<body>
    <form method="POST">
        <div>

            <h1 class="left">[____@CodeAnon ~]$ username:</h1>
            <input class="left" type="terminal" autofocus autocomplete="off" name="registerUsername">

        </div>
        <div class="clear">

            <h1 class="left">[____@CodeAnon ~]$ email:</h1>
            <input class="left" type="terminal" autofocus autocomplete="off" name="registerEmail">

        </div>
        <div class="clear">

            <h1 class="left">[____@CodeAnon ~]$ confirm email:</h1>
            <input class="left" type="terminal" autofocus autocomplete="off" name="registerConfirmEmail">

        </div>
        <div class="clear">

            <h1 class="left">[____@CodeAnon ~]$ password:</h1>
            <input class="left" type="password" autofocus autocomplete="off" name="registerPassword">

        </div>
        <div class="clear">

            <h1 class="left">[____@CodeAnon ~]$ confirm password:</h1>
            <input class="left" type="password" autofocus autocomplete="off" name="registerConfirmPassword">

        </div>
        <div class="clear error-msg">
            <b><%= registerFailMsg %></b>
        </div>
        <div class="clear">
            <button><b>Register</b></button>
        </div>
    </form>
    <div class="clear">
        <button><a href="../">/</a></button>
    </div>
    <div class="clear">
        <button><a href="../login">/login</a></button>
    </div>
</body>

</html>